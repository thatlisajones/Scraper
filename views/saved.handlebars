<div class="card card-fill">
    <h5 class="card-header">{{title}}</h5>
    <div class="card-body">
        <center>
        <div class="row date-Btn">
            <div class="col-1"></div>
            <div class="col">
                <h5 class="card-text">{{date}}</h5>
            </div>      
            <div class="col">
                <a href="{{link}}" class="btn btn-info keep-Btn card-link">read more</a>
            </div>
            <div class="col">
                <button type="button" class="btn btn-info mx-auto remove" data-id={{this._id}}>delete</button>
            </div>
            <div class="col">    
                <button type="button" class="btn btn-info mx-auto addNote" data-toggle="modal" data-target="#noteModal{{this._id}}" data-id={{this._id}}>note</button>
            </div>
            <div class="col-1"></div>
        </div>
        <div class="row">
            <div class="col">
                <img class="card-img img-fluid" src="{{img}}">
            </div>
        </div>
        </center>
    </div>
